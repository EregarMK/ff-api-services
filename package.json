    "@types/qs": "^6.5.1",
    "@types/storejs": "^1.3.1",
    "@types/uuid": "^3.4.4",
    "aws-amplify": "1.1.19",
    "axios": "^0.18.0",
    "detect-node": "^2.0.3",
    "qs": "^6.5.2",
    "store": "^2.0.12",
    "tslib": "^1.9.3",
    "wpapi": "1.1.2"
  },
  "devDependencies": {
    "amazon-cognito-identity-js": "3.0.9",
    "recursive-search": "^1.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typedoc": "0.14.2",
    "typescript": "^3.0.3"
  }
