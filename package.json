{
  "name": "@flowfact/api-services",
  "version": "10.0.0-SNAPSHOT",
  "description": "middleware of flowfact to interact between frontend and backend",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "repository": "ssh://git@github.com:FLOWFACTCorp/ff-api-services.git",
  "author": "FLOWFACT GmbH",
  "license": "SEE LICENSE IN LICENSE",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://arm.flowfact-ops.cloud/content/repositories/ffnpm/"
  },
  "scripts": {
    "build:cjs": "tsc",
    "build:esm": "tsc --project tsconfig.esm.json",
    "rebuild:cjs": "rimraf lib/ && npm run build:cjs",
    "rebuild:esm": "rimraf es/ && npm run build:esm",
    "prepare": "npm run rebuild:cjs && npm run rebuild:esm",
    "lint": "tslint --project .",
    "doc": "typedoc --out doc/"
  },
  "dependencies": {
    "@flowfact/consul-client": "0.20190322.4",
